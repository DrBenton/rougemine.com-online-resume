{
  "name": "rougemine.com-react",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/*",
    "api:dump": "cd api && npm run dump && cd -",
    "ssr:webpack": "cd webapp-ssr && npm run webpack && cd -",
    "ssr:render": "cd webapp-ssr && npm run render && cd -",
    "ssr:dev:watch": "cd webapp-ssr && npm run watch && cd -",
    "webapp:dev:start":
      "webpack-dev-server --config webapp/webpack/config.browser.dev.js --open",
    "webapp:dev:watch":
      "webpack --progress --watch --config webapp/webpack/config.browser.dev.js",
    "webapp:dev:build": "webpack --config webapp/webpack/config.browser.dev.js",
    "webapp:prod:build":
      "webpack --config webapp/webpack/config.browser.prod.js",
    "prettier:api": "prettier --write 'api/**/*.js' ",
    "prettier:webapp": "prettier --write 'webapp/**/*.js' ",
    "prettier:webapp-ssr": "prettier --write 'webapp-ssr/**/*.js' ",
    "prettier":
      "npm run prettier:api && npm run prettier:webapp && npm run prettier:webapp-ssr",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "date-fns": "^1.29.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "1.7.4",
    "redux-devtools": "^3.4.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-monitor": "^1.0.13",
    "webpack-shell-plugin": "^0.5.0"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  }
}
