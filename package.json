{
  "name": "rougemine.com-react",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ssr:webpack": "webpack --config webapp/webpack/webpack.config.node.js",
    "ssr:render:en": "node webapp/bin/generate-static.js 'en'",
    "ssr:render:fr": "node webapp/bin/generate-static.js 'fr'",
    "ssr:render":
      "npm run --silent ssr:render:en > dist/index.en.html && npm run --silent ssr:render:fr > dist/index.fr.html ",
    "ssr:watch":
      "webpack --watch --config webapp/webpack/webpack.config.node.js",
    "ssr:build": "webpack --config webapp/webpack/webpack.config.node.js",
    "webapp:dev:watch":
      "webpack --watch --config webapp/webpack/webpack.config.dev.js",
    "webapp:dev:build": "webpack --config webapp/webpack/webpack.config.dev.js",
    "prettier:api": "prettier --write 'api/**/*.js' ",
    "prettier:webapp": "prettier --write 'webapp/**/*.js' ",
    "prettier": "npm run prettier:api && npm run prettier:webapp",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "1.7.4",
    "redux-devtools": "^3.4.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  }
}
