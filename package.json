{
  "name": "rougemine.com-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:node": "webpack --config webpack/webpack.config.node.js",
    "render": "npm run build:node && babel-node dist/bundle.node.js",
    "watch":
      "cp src/index.html dist/ && webpack --watch --config webpack/webpack.config.dev.js",
    "build":
      "cp src/index.html dist/ && webpack --config webpack/webpack.config.dev.js",
    "prettier": "prettier --write \"src/*.js\" \"src/**/*.js\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "1.7.4",
    "redux-devtools": "^3.4.0"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  }
}
